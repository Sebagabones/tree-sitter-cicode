{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "cicode",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "whitespace": {
      "type": "PATTERN",
      "value": "\\s"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "//"
              },
              {
                "type": "PATTERN",
                "value": ".*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "!"
              },
              {
                "type": "PATTERN",
                "value": ".*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "/*"
              },
              {
                "type": "PATTERN",
                "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
              },
              {
                "type": "STRING",
                "value": "*/"
              }
            ]
          }
        ]
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "function_definition"
        }
      ]
    },
    "function_scope": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "PUBLIC"
        },
        {
          "type": "STRING",
          "value": "PRIVATE"
        }
      ]
    },
    "_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "INT"
        },
        {
          "type": "STRING",
          "value": "STRING"
        },
        {
          "type": "STRING",
          "value": "REAL"
        },
        {
          "type": "STRING",
          "value": "QUALITY"
        },
        {
          "type": "STRING",
          "value": "TIMESTAMP"
        },
        {
          "type": "STRING",
          "value": "BOOL"
        }
      ]
    },
    "function_types": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_type"
        },
        {
          "type": "STRING",
          "value": "VOID"
        }
      ]
    },
    "function_name": {
      "type": "PATTERN",
      "value": "[0-9A-Za-z]+"
    },
    "function_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "function_scope"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "function_types"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "FUNCTION"
        },
        {
          "type": "SYMBOL",
          "name": "function_name"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "STRING",
      "value": "\r"
    },
    {
      "type": "SYMBOL",
      "name": "whitespace"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}