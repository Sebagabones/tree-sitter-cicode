{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "cicode",
      "camelcase": "Cicode",
      "title": "Cicode",
      "scope": "source.cicode",
      "file-types": ["ci"],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^cicode$",
      "class-name": "TreeSitterCicode"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A language that interfaces with Citect/PlantSCADA",
    "authors": [
      {
        "name": "Sebastian Gazey",
        "url": "https://mahoosively.gay/"
      }
    ],
    "links": {
      "repository": "https://github.com/Sebagabones/tree-sitter-cicode"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
